# === 基础TARGET配置 ===
CONFIG_TARGET_qualcommax=y
CONFIG_TARGET_qualcommax_ipq60xx=y
CONFIG_TARGET_MULTI_PROFILE=y


# === 设备启用 ===
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_jdcloud_re-ss-01=y
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_jdcloud_re-cs-02=y

# === 包组定义，仅展示分组逻辑，无实际影响 ===
CONFIG_PACKAGE_GROUP_BASE="netspeedtest openclash samba4 smbuser tailscale taskplan ttyd upnp zram-swap"
CONFIG_PACKAGE_GROUP_ATHENA_LITE="easytier lucky partexp"
CONFIG_PACKAGE_GROUP_ATHENA_FULL="ddns-go homeproxy istorex"

# === 亚瑟系列配置(特定包会默认安装)) ===
# 亚瑟Lite
CONFIG_TARGET_DEVICE_PACKAGES_qualcommax_ipq60xx_DEVICE_jdcloud_re-ss-01_lite=""
# 亚瑟Full(与雅典娜Lite相同)
CONFIG_TARGET_DEVICE_PACKAGES_qualcommax_ipq60xx_DEVICE_jdcloud_re-ss-01="easytier lucky partexp"

# === 雅典娜系列配置(特定包会默认安装)) ===
# 雅典娜Lite(与亚瑟Full相同)
CONFIG_TARGET_DEVICE_PACKAGES_qualcommax_ipq60xx_DEVICE_jdcloud_re-cs-02_lite="luci-app-easytier luci-app-lucky luci-app-partexp"
# 雅典娜Full
CONFIG_TARGET_DEVICE_PACKAGES_qualcommax_ipq60xx_DEVICE_jdcloud_re-cs-02="luci-app-easytier luci-app-lucky luci-app-partexp luci-app-ddns-go luci-app-homeproxy luci-app-istorex"

# === 设备特定包(特定包会默认安装)) ===
CONFIG_TARGET_DEVICE_PACKAGES_qualcommax_ipq60xx_DEVICE_jdcloud_re-ss-01+=""
CONFIG_TARGET_DEVICE_PACKAGES_qualcommax_ipq60xx_DEVICE_jdcloud_re-cs-02+="luci-app-athena-led luci-i18n-athena-led-zh-cn"

# === 启用子配置 ===
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_jdcloud_re-ss-01_lite=y
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_jdcloud_re-cs-02_lite=y


# === 编译优化 ===
CONFIG_DEVEL=y
CONFIG_TARGET_OPTIONS=y
CONFIG_TARGET_OPTIMIZATION="-O3 -pipe -march=armv8-a+crypto+crc -mcpu=cortex-a53+crypto+crc -mtune=cortex-a53"


# === NSS配置 ===
CONFIG_ATH11K_MEM_PROFILE_512M=y
CONFIG_NSS_FIRMWARE_VERSION_12_5=y

# 雅典娜LED
#CONFIG_PACKAGE_luci-app-athena-led=m
#CONFIG_PACKAGE_luci-i18n-athena-led-zh-cn=m

